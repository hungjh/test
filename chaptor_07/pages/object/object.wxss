
.table {
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: right;
  height: 3rem;
  align-items: right;
}
.td1 {
    width:30%;
    justify-content: right;
    text-align: right;
}
.td2 {
    width:60%;
    justify-content: right;
    text-align: right;
}
