.content {
  width: 100%;
}

.product {
  margin-bottom: 10px;
}

.product .ulli{
  height: 45px;
  border-bottom: 1px solid #dbdbdb;
  padding: 0px 20px;
  cursor: pointer;
}

.product .ulli.active {
  background: #eee;
}

.product .ulli image {
  height: 25px;
  width: 25px;
  vertical-align: middle;
}
.product .ulli text {
  height: 45px;
  vertical-align: middle;
  line-height: 45px;
  padding-left: 10px;
  font-size:14px;
}
.risk_title {
  padding-right: 15px;
  padding-left: 15px;
}
.risk_title .btn-back {
  width: 100%;
  margin: 10px auto;
  border-radius: 20px;
  color: #fff;
  background: #1098ec;
  line-height: 25px;
  padding: 6px 12px;
}
