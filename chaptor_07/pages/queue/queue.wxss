page{
  height:100%
}
.process-init {
  padding-top: 10px;
  text-align: center
}

.process-init image {
  width: 300px;
  height: 230px;
}

.loadingText {
  font-size: 17px;
  padding: 10rpx;
  text-align: center;
}

.loadingSpan {
  width: 30px;
  vertical-align: middle;
  display: inline-block;
}

.typing_loader {
  width: 6px;
  height: 6px;
  border-radius: 50%;
  -webkit-animation: typing 1s linear infinite alternate;
  -moz-animation: Typing 1s linear infinite alternate;
  animation: typing 1s linear infinite alternate;
  margin: 0px auto;
  position: relative;
  left: -11px;
}

@keyframes typing {
  0% {
    background-color: rgba(2, 138, 255, 1);
    box-shadow: 12px 0px 0px 0px rgba(2, 138, 255, 0.2), 24px 0px 0px 0px rgba(2, 138, 255, 0.2);
  }

  25% {
    background-color: rgba(2, 138, 255, 0.4);
    box-shadow: 12px 0px 0px 0px rgba(2, 138, 255, 2), 24px 0px 0px 0px rgba(2, 138, 255, 0.2);
  }

  75% {
    background-color: rgba(2, 138, 255, 0.4);
    box-shadow: 12px 0px 0px 0px rgba(2, 138, 255, 0.2), 24px 0px 0px 0px rgba(2, 138, 255, 1);
  }
}

.process-queue {
  padding: 10px;
  padding-top:40px;
}

.wait-content {
  text-align: center;
  width: 100%;
  height: 100%;
  position: relative;
}

.wait-content .clock {
  width: 200px;
  height: 200px;
  margin: 0 auto;
  position: relative;
}

.wait-content .clock image {
  width: 100%;
  height: 100%;
}

.wait-content .clock text {
  color: #fff;
  font-size: 24px;
  margin: 0px;
  line-height: 20px;
  user-select: none;
  -ms-user-select: none;
  cursor: default;
  text-align: center;
  position: absolute;
  bottom: 30px;
  left: 50%;
  margin-left: -30px;
}

.cancel-queue-button {
  position: fixed;
  left: 0;
  bottom: 0;
  height: 50px;
  width: 100%;
  font-size: 17px;
  color: #fff;
  background: rgb(0, 126, 206);
  border: none;
  border-radius: 0px;
  line-height: 50px;
}

.wait-content text {
  font-size: 15px;
  line-height: 30px;
  margin-top: 25px;
  text-align: center;
}
.process-video {
    width: 100%;
    height: 100%;
    background:#000;
    position:relative
}
.Agent-Area {
    width: 160px;
    height: 120px;
    position: absolute;
    bottom: 0px;
    right: 0px;
    background-color: #004790;
    z-index: 2;
}
.Agent-Area>image {
    width: 70px;
    height: 84px;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -42px;
    margin-left: -35px;
    z-index: 222;
}
#Agent-Area {
    width: 180px;
    height: 135px;
    position: absolute;
    bottom: 0;
    right: 0;
    z-index: 1000;
}
.Client-Area {
    width: 100%;
    height: calc(100% - 135px);
    background: #004790;
    position: absolute;
    top:0px;
}
.Client-Area>image {
    width: 100px;
    height: 120px;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -60px;
    margin-left: -50px;
}
#Client-Area {
    width: 100%;
    height: 100%;
    position: relative;
}
#Question-Content {
    overflow-y: auto;
    width: calc(100% - 180px);
    height: 135px;
    position:fixed;
    bottom: 0;
    left: 0;
    color: #fff;
    z-index: 1010;
}
#opacity-bg{
    width: calc(100% - 180px);
    height: 135px;
    position:fixed;
    bottom: 0;
    left: 0;
    opacity: 0.4;
    z-index: 1009;
    background-color: rgb(0, 126, 206);
}
.cover-view-box{
  width:100%;
  height:100%
}
